per100recallbySOA$total_recall_list<-(per100recallbySOA$total_recall_list)/14*100 #Now we have the % of items recalled
listrecallSOAplot2<-per100recallbySOA %>%
group_by(SOA) %>%
get_summary_stats(total_recall_list,type='mean_ci')
